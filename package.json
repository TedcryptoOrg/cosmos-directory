{
  "name": "@tedcryptoorg/cosmos-directory",
  "version": "0.1.2",
  "description": "Cosmos Directory in Typescript",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "prepack": "npm run build"
  },
  "keywords": [],
  "author": "Josh_Lopes<josluis.lopes@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/TedcryptoOrg/cosmos-directory",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "dependencies": {
    "axios": "^1.2.5",
    "compare-versions": "^6.0.0-rc.1",
    "cosmjs-types": "^0.9.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.6",
    "babel-jest": "^29.3.1",
    "eslint-config-love": "^101.0.0",
    "jest": "^29.3.1",
    "jest-circus": "^29.6.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/cjs/types/index.d.ts"
    }
  },
  "files": [
    "lib/**/*"
  ]
}
